    "on-click": "$TERMINAL --hold -e duf"
  },
  "systemd-failed-units#system": {
    "hide-on-ok": true,
    "format": "‚úó {nr_failed}",
    "format-ok": "‚úì",
    "system": true,
    "user": false,
    "on-click": "$TERMINAL --hold -e systemctl --failed",
  },
  "systemd-failed-units#user": {
    "hide-on-ok": true,
    "format": "‚úó {nr_failed}",
    "format-ok": "‚úì",
    "system": false,
    "user": true,
